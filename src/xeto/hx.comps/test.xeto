// Contains a slot for each status flag
StatusTestComp : HxComp <icon:"flask-conical", group:@ion.misc::group-util> {
  ok:         StatusNumber <summary, readonly>
  alarm:      StatusNumber <summary, readonly>
  disabled:   StatusNumber <summary, readonly>
  down:       StatusNumber <summary, readonly>
  fault:      StatusNumber <summary, readonly>
  nullVal:    StatusNumber <summary, readonly>
  overridden: StatusNumber <summary, readonly>
  stale:      StatusNumber <summary, readonly>
  unacked:    StatusNumber <summary, readonly>
}

