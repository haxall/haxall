    { lib: "sys",     versions: BuildVar "ph.depend" }
    { lib: "axon",    versions: BuildVar "hx.depend" }
    { lib: "hx",      versions: BuildVar "hx.depend" }
    { lib: "hx.conn", versions: BuildVar "hx.depend" }
