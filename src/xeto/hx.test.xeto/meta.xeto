+Spec {
  q: Date?
  r: Date?
  s: Date?
  foo: Str?
  bar: Str?
  qux: Str?

  metaQ: Marker?
  metaR: Marker? <noInherit>
  metaNum: Number?
}

MetaInheritA: Dict <metaQ, metaR> {
  x: Str <metaQ, metaR>
  y: Str <metaQ, metaR>
}

MetaInheritB: MetaInheritA <metaNum:"123"> {
  y: Str
}

MetaInheritAltA: Dict {
  <metaQ>
  <metaR>
}

MetaInheritAltB: MetaInheritAltA {
  <metaNum:"123">
}

