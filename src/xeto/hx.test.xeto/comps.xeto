TestFolder : Comp {
}

TestAdd : Comp {
  in1: TestVal
  in2: TestVal
  out: TestVal <transient>
}

TestNumberAdd : Comp {
  in1: Number
  in2: Number
  out: Number <transient>
}

TestRamp : Comp {
  out: TestVal <transient>
}

TestCounter : Comp {
  out: TestVal <transient>
}

TestComposite : Comp {
  descr: "test descr"
  dur: Duration "5min"
  a: TestAdd {
     in1: {val:7}
     in2: {val:5}
  }
  nest: TestFolder {
    b: TestAdd {
       in1: {val:17}
       in2: {val:15}
    }
  }
}

TestFoo: Comp {
  a: Str "alpha"
  b: Str "beta"
  c: Str?
  method1: Func { arg: Str }
  method2: Func { returns: Str }
  method3: Func { arg: Str, returns: Str }
}

TestVal : Dict {
  val: Number
  status: Str
}

